	</tag>

	<tag>
		<name>show</name>
		<tagclass>com.primeton.report.client.api.tag.ShowTask</tagclass>
		<info>
			show report that generate with generateHtml / generateExcel / generatePdf Tag 
			or BL_generateHtml/ BL_generateExcel / BL_generatePdf
		</info>
		<attribute>
			<name>taskName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>downloadFileName</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>closeTaskAndClear</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

	</tag>

	<tag>
		<name>showApplet</name>
		<tagclass>
			com.primeton.report.client.api.tag.ShowAppletTag
		</tagclass>
		<info>
			show report that generate with runReport Tag or BL_runReport
		</info>
		<attribute>
			<name>taskName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>closeTaskAndClear</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>silent</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>appletName</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>previewEditable</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>showPreviewToolbar</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>autoScale</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>zoomLocked</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<tag>
		<name>showHtml</name>
		<tagclass>
			com.primeton.report.client.api.tag.ShowHtmlTag
		</tagclass>
		<info>
			show report using HTML format that generate with runReport
			Tag or BL_runReport
		</info>
