	<tag>
		<name>showPdf</name>
		<tagclass>
			com.primeton.report.client.api.tag.ShowPdfTag
		</tagclass>
		<info>
			show report using PDF format that generate with runReport
			Tag or BL_runReport
		</info>
		<attribute>
