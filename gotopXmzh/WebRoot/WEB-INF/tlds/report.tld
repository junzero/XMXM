	<tag>
		<name>close</name>
		<tagclass>
			com.primeton.report.client.api.tag.CloseTask
		</tagclass>
		<info>
			close report Task that generate with runReport Tag or
			BL_runReport
		</info>
		<attribute>
			<name>taskName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>clearOutputFile</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<tag>
		<name>taskInfo2TaskName</name>
		<tagclass>
			com.primeton.report.client.api.tag.TaskInfo2TaskName
		</tagclass>
		<info>
			save ReportSession that generate with runReport Tag or
			BL_runReport in HTTP Session
		</info>
		<attribute>
			<name>sessionName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>taskName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>


</taglib>