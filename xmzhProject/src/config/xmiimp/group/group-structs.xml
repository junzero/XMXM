<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
  <package extends="default" name="group" namespace="/group">
    <action class="tGroupAction" method="{1}" name="tGroupAction_*">
      <result name="viewlist">
        /jsp/group/tGroupDatacell.jsp
      </result>
      <!-- 我的群组列表 -->
       <result name="myGroupList">
        /jsp/group/myGroupList.jsp
      </result>
      <!-- 新增群组 -->
       <result name="group">
        /jsp/group/group.jsp
      </result>
       
    </action>
  
    <action class="tGroupmemberAction" method="{1}" name="tGroupmemberAction_*">
      <result name="viewlist">
        /jsp/group/tGroupmemberDatacell.jsp
      </result>
    </action>
    </package>
</struts>