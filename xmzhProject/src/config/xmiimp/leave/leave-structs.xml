<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
  <package extends="default" name="leave" namespace="/leave">
    <action class="tApplyLeaveAction" method="{1}" name="tApplyLeaveAction_*">
      <result name="viewlist">
        /jsp/leave/tApplyLeaveDatacell.jsp
      </result>
      <result name="queryAll">
      	/jsp/leave/leave_list.jsp
      </result>
      <result name="info">
      	/jsp/leave/leave_approve.jsp
      </result>
      <result name="addSuccess">
      /jsp/leave/next_state.jsp
      </result>
      <result name="getOldLeave">
      /jsp/leave/leave_add.jsp
      </result>
    </action>
    </package>
</struts>