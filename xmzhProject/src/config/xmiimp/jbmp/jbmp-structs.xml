<struts>
  <package extends="default" name="jbmp" namespace="/jbmp">
    <action class="jbpm4HistActinstAction" method="{1}" name="jbpm4HistActinstAction_*">
      <result name="viewlist">
        /jsp/jbmp/jbpm4HistActinstDatacell.jsp
      </result>
    </action>
  
    <action class="jbpm4HistTaskAction" method="{1}" name="jbpm4HistTaskAction_*">
      <result name="viewlist">
